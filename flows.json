    },
    {
        "id": "20858e90a33a79d0",
        "type": "function",
        "z": "1c38bb45ab2e35d1",
        "name": "Set plate remain flow value",
        "func": "const old_plate_remain = flow.get(\"plate_remain\")\nflow.set(\"plate_remain\", msg.payload.num)\n\nvar eaten_amount = flow.get(\"eaten_amount\")\nif(old_plate_remain - msg.payload.num > 0){\n    eaten_amount = parseFloat((eaten_amount + old_plate_remain - msg.payload.num).toFixed(2))\n    flow.set(\"eaten_amount\", eaten_amount)\n}\n\nmsg.payload = eaten_amount\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 660,
        "y": 200,
        "wires": [
